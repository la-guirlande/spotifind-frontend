{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {},
    "jsx-runtime": {
        "plugins": [
          "react"
        ],
        "parserOptions": {
          "ecmaFeatures": {
            "jsx": true
          },
          "jsxPragma": null // for @typescript/eslint-parser
        },
        "rules": {
          "react/react-in-jsx-scope": 0,
          "react/jsx-uses-react": 0
        }
      }
}
